  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "hardhat": "^2.24.0",
